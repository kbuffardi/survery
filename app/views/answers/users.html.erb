
<h4> Users Created: <%= @users.size %></h3>
<%= link_to "Back", :back %>
<div class="vert-space-sm"></div>
</tr>
<div class="panel panel-default">
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>
          Email
        </th>
        <th>
          Created At
        </th>
        <th>
          Current Sign in IP
        </th>
        <th>
          Last Sign in IP
        </th>
        <th>
          Sign in Count
        </th>
        <th>
          Current Sign in at
        </th>
        <th>
          Last Sign in at
        </th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |u| %>
        <tr>
          <td>
            <%= u.email %>
          </td>
          <td>
              <%= u.created_at %>
          </td>
          <td>
            <%= u.current_sign_in_ip %>
          </td>
          <td>
            <%= u.last_sign_in_ip %>
          </td>
          <td>
            <%= u.sign_in_count %>
          </td>
          <td>
            <%= u.current_sign_in_at %>
          </td>
          <td>
            <%= u.last_sign_in_at %>
          </td>
      </tr>
        <% end %>
    </tbody>
  </table>
</div>
