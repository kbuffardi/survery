<div class="progress">
  <div class="progress-bar progress-bar-info progress-bar-striped" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100" style="width: 10%">
    <span class="sr-only">10% Complete (success)</span>
  </div>
</div>

<h3>Which of these foods do you feel are problematic for you?</h3>

<div class="selections col-md-offset-1 checkbox checkbox-primary">
<%= form_for @survey do |f| %>
	<% @categories.each do |category| %>
		<h4><%= category.name %></h4>
<div style="margin-left: 16px">
		<%= f.collection_check_boxes :food_ids, category.foods.all, :id, :name, {}, :multiple => true %><br>
    </div>
	<% end %>
	<%= f.submit 'Next', :class => "btn btn-primary" %>

<% end %>
</div>

<script>
$( document ).ready(function() {
  $( "#e1" ).select2();
});
</script>
