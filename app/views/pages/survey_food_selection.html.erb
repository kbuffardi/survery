<div class="progress">
  <div class="progress-bar progress-bar-info progress-bar-striped" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
    <span class="sr-only">40% Complete (success)</span>
  </div>
</div>

<%= form_for(@survey) do |f| %>
	<% @categories.each do |category| %>
		<p>Which of these foods of category <%= category.name %> do you feel a lack of control around?</p>

		Foods <br>
		<%= f.collection_check_boxes :food_ids, category.foods.all, :id, :name, {}, :multiple => true %><br>
	<% end %>		
	<%= f.submit %>
<% end %>

<script>
$( document ).ready(function() {
  $( "#e1" ).select2();
});
</script>