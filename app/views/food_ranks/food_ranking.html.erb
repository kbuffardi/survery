<div class="progress">
  <div class="progress-bar progress-bar-info progress-bar-striped" role="progressbar" aria-valuenow="15" aria-valuemin="0" aria-valuemax="100" style="width: 15%">
    <span class="sr-only">15% Complete (success)</span>
  </div>
</div>

<div class="selections">
<%= form_for(@survey) do |f| %>
<p>Rank your selected foods from most problematic (1) to least problematic.</p>

  <% @foods.each do |food| %>

		<%= f.collection_select :food_ids, @foods, :id, :id, :multiple => true %>
    <%= food.name %> <br>
	<% end %>

	<%= f.submit 'Next', :class => "btn btn-primary" %>

<% end %>
</div>
